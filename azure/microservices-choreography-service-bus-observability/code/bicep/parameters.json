{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environmentName": {
      "value": "dev"
    },
    "baseName": {
      "value": "choreography"
    },
    "uniqueSuffix": {
      "value": "[uniqueString(resourceGroup().id, deployment().name)]"
    },
    "serviceBusCapacity": {
      "value": 1
    },
    "containerAppsMinReplicas": {
      "value": 1
    },
    "containerAppsMaxReplicas": {
      "value": 10
    },
    "functionAppConsumptionPlanLocation": {
      "value": "eastus"
    },
    "containerImage": {
      "value": "mcr.microsoft.com/azuredocs/containerapps-helloworld:latest"
    },
    "enableApplicationInsights": {
      "value": true
    },
    "enableWorkbooks": {
      "value": true
    },
    "enableZoneRedundancy": {
      "value": false
    },
    "logRetentionInDays": {
      "value": 30
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Application": "microservices-choreography",
        "Purpose": "event-driven-architecture",
        "CostCenter": "IT",
        "CreatedBy": "bicep-deployment"
      }
    }
  }
}