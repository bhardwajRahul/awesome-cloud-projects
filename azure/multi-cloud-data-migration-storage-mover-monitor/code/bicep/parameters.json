{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "storageAccountName": {
      "value": "stmigrationdemo123"
    },
    "storageMoverName": {
      "value": "sm-migration-demo"
    },
    "logAnalyticsWorkspaceName": {
      "value": "log-migration-demo"
    },
    "logicAppName": {
      "value": "logic-migration-demo"
    },
    "awsS3BucketName": {
      "value": "your-source-s3-bucket"
    },
    "awsAccountId": {
      "value": "123456789012"
    },
    "awsRegion": {
      "value": "us-east-1"
    },
    "adminEmail": {
      "value": "admin@yourcompany.com"
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "storageAccountAccessTier": {
      "value": "Hot"
    },
    "logAnalyticsRetentionDays": {
      "value": 30
    },
    "logAnalyticsPricingTier": {
      "value": "pergb2018"
    },
    "enableHierarchicalNamespace": {
      "value": false
    },
    "migrationContainerName": {
      "value": "migrated-data"
    },
    "tags": {
      "value": {
        "purpose": "migration",
        "environment": "demo",
        "source": "aws-s3",
        "migration-type": "s3-to-blob",
        "project": "multi-cloud-migration",
        "owner": "infrastructure-team",
        "cost-center": "IT-Operations",
        "backup-enabled": "true",
        "monitoring-enabled": "true",
        "compliance-level": "standard"
      }
    }
  }
}