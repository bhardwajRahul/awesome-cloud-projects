{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "environment": {
      "value": "dev"
    },
    "applicationName": {
      "value": "webapp"
    },
    "vnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnetAddressPrefix": {
      "value": "10.0.1.0/24"
    },
    "vmSkuSize": {
      "value": "Standard_B2s"
    },
    "initialInstanceCount": {
      "value": 2
    },
    "minInstanceCount": {
      "value": 2
    },
    "maxInstanceCount": {
      "value": 10
    },
    "adminUsername": {
      "value": "azureuser"
    },
    "sshPublicKey": {
      "value": "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAABAQC7laRyN... (replace with your SSH public key)"
    },
    "enableApplicationInsights": {
      "value": true
    },
    "scaleOutCpuThreshold": {
      "value": 70
    },
    "scaleInCpuThreshold": {
      "value": 30
    },
    "resourceTags": {
      "value": {
        "Environment": "dev",
        "Application": "webapp",
        "ManagedBy": "Bicep",
        "Purpose": "AutoScalingWebApp",
        "Project": "RecipeDemo",
        "Owner": "DevOpsTeam",
        "CostCenter": "Engineering"
      }
    }
  }
}