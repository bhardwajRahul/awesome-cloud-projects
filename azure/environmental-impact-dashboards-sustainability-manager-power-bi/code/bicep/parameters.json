{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "dev"
    },
    "location": {
      "value": "eastus"
    },
    "projectName": {
      "value": "sustain"
    },
    "resourceOwner": {
      "value": "SustainabilityTeam"
    },
    "costCenter": {
      "value": "Environmental"
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "logAnalyticsWorkspaceSku": {
      "value": "PerGB2018"
    },
    "functionAppPlanSku": {
      "value": "Y1"
    },
    "enableApplicationInsights": {
      "value": true
    },
    "enableDataRefresh": {
      "value": true
    },
    "dataRefreshSchedule": {
      "value": "0 0 6 * * *"
    },
    "enableBudgetAlerts": {
      "value": true
    },
    "monthlyBudgetThreshold": {
      "value": 500
    },
    "budgetAlertEmails": {
      "value": [
        "sustainability-team@company.com"
      ]
    },
    "enableAdvancedThreatProtection": {
      "value": false
    },
    "enablePrivateEndpoints": {
      "value": false
    },
    "vnetResourceId": {
      "value": ""
    },
    "subnetResourceId": {
      "value": ""
    }
  }
}