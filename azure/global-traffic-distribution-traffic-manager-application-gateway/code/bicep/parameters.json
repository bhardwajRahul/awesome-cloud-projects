{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "primaryLocation": {
      "value": "eastus"
    },
    "secondaryLocation": {
      "value": "uksouth"
    },
    "tertiaryLocation": {
      "value": "southeastasia"
    },
    "uniqueSuffix": {
      "value": "[uniqueString(resourceGroup().id)]"
    },
    "adminUsername": {
      "value": "azureuser"
    },
    "sshPublicKey": {
      "value": "ssh-rsa @example.com"
    },
    "vmssInstanceCount": {
      "value": 2
    },
    "vmssSkuName": {
      "value": "Standard_B2s"
    },
    "appGatewayCapacity": {
      "value": 2
    },
    "trafficManagerTtl": {
      "value": 30
    },
    "environmentTag": {
      "value": "production"
    },
    "purposeTag": {
      "value": "global-traffic-distribution"
    }
  }
}