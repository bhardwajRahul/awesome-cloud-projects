{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "deploy-auto"
    },
    "location": {
      "value": "East US"
    },
    "environment": {
      "value": "dev"
    },
    "containerImage": {
      "value": "mcr.microsoft.com/azure-cli:latest"
    },
    "cpuCores": {
      "value": "0.5"
    },
    "memorySize": {
      "value": "1.0Gi"
    },
    "cronExpression": {
      "value": "0 2 * * *"
    },
    "enableKeyVault": {
      "value": true
    },
    "enableScheduledJob": {
      "value": true
    }
  }
}