{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "pwa"
    },
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "eastus"
    },
    "repositoryUrl": {
      "value": "https://github.com/yourusername/your-pwa-repo"
    },
    "repositoryBranch": {
      "value": "main"
    },
    "repositoryToken": {
      "value": ""
    },
    "appLocation": {
      "value": "src"
    },
    "apiLocation": {
      "value": "src/api"
    },
    "outputLocation": {
      "value": "src"
    },
    "staticWebAppSku": {
      "value": "Free"
    },
    "cdnSku": {
      "value": "Standard_Microsoft"
    },
    "appInsightsPricingTier": {
      "value": "Basic"
    },
    "enableCdnCompression": {
      "value": true
    },
    "compressionContentTypes": {
      "value": [
        "application/javascript",
        "application/json",
        "application/xml",
        "text/css",
        "text/html",
        "text/javascript",
        "text/plain",
        "text/xml",
        "image/svg+xml",
        "application/font-woff",
        "application/font-woff2"
      ]
    },
    "staticAssetsCacheDuration": {
      "value": "24:00:00"
    },
    "dynamicContentCacheDuration": {
      "value": "1:00:00"
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Application": "PWA",
        "ManagedBy": "Bicep",
        "Recipe": "building-progressive-web-apps-with-azure-static-web-apps-and-cdn",
        "Owner": "DevTeam",
        "CostCenter": "Engineering",
        "Project": "PWA-Demo"
      }
    }
  }
}