{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourcePrefix": {
      "value": "containerworkflow"
    },
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "serviceBusSkuName": {
      "value": "Standard"
    },
    "containerRegistrySku": {
      "value": "Basic"
    },
    "maxExecutions": {
      "value": 10
    },
    "minExecutions": {
      "value": 0
    },
    "replicaTimeout": {
      "value": 1800
    },
    "maxRetryCount": {
      "value": 3
    },
    "queueMessageCountThreshold": {
      "value": 1
    },
    "pollingIntervalSeconds": {
      "value": 30
    },
    "containerImage": {
      "value": "mcr.microsoft.com/azuredocs/containerapps-helloworld:latest"
    },
    "containerCpu": {
      "value": "0.5"
    },
    "containerMemory": {
      "value": "1Gi"
    },
    "enableMonitoring": {
      "value": true
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Project": "ContainerAppsWorkflow",
        "ManagedBy": "Bicep",
        "Owner": "DevOps Team",
        "CostCenter": "Engineering",
        "Application": "EventDrivenProcessing"
      }
    }
  }
}