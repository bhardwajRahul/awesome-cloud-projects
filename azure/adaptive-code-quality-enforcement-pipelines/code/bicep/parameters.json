{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "uniqueSuffix": {
      "value": "demo001"
    },
    "environment": {
      "value": "dev"
    },
    "appInsights": {
      "value": {
        "name": "ai-quality-demo001",
        "kind": "web",
        "applicationType": "web",
        "retentionInDays": 90,
        "samplingPercentage": 100
      }
    },
    "logicApps": {
      "value": {
        "name": "la-quality-feedback-demo001",
        "qualityThreshold": 80,
        "enableDiagnostics": true
      }
    },
    "storageAccount": {
      "value": {
        "name": "stqualitydemo001",
        "sku": "Standard_LRS",
        "kind": "StorageV2",
        "accessTier": "Hot",
        "enableBlobEncryption": true,
        "enableFileEncryption": true
      }
    },
    "logAnalytics": {
      "value": {
        "name": "law-quality-demo001",
        "sku": "PerGB2018",
        "retentionInDays": 30
      }
    },
    "keyVault": {
      "value": {
        "name": "kv-quality-demo001",
        "sku": "standard",
        "enabledForDeployment": false,
        "enabledForDiskEncryption": false,
        "enabledForTemplateDeployment": true,
        "enableSoftDelete": true,
        "softDeleteRetentionInDays": 7
      }
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Purpose": "CodeQualityPipeline",
        "ManagedBy": "Bicep",
        "Recipe": "IntelligentCodeQuality",
        "Owner": "DevOps Team",
        "CostCenter": "Engineering",
        "Project": "Quality Automation"
      }
    }
  }
}