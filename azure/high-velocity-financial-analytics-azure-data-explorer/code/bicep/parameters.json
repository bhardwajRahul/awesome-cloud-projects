{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourcePrefix": {
      "value": "fmarket"
    },
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "owner": {
      "value": "finance-team"
    },
    "adxClusterSku": {
      "value": {
        "name": "Dev(No SLA)_Standard_D11_v2",
        "tier": "Basic",
        "capacity": 1
      }
    },
    "eventHubsConfig": {
      "value": {
        "sku": "Standard",
        "throughputUnits": 2,
        "maxThroughputUnits": 10,
        "autoInflateEnabled": true
      }
    },
    "functionAppConfig": {
      "value": {
        "runtime": "python",
        "runtimeVersion": "3.9",
        "functionsVersion": "4"
      }
    },
    "databaseConfig": {
      "value": {
        "softDeletePeriod": 30,
        "hotCachePeriod": 7
      }
    },
    "marketDataPartitions": {
      "value": 8
    },
    "tradingEventsPartitions": {
      "value": 4
    },
    "enableApplicationInsights": {
      "value": true
    },
    "enableLogAnalytics": {
      "value": true
    }
  }
}