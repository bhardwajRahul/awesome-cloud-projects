{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "resourceSuffix": {
      "value": "prod001"
    },
    "environment": {
      "value": "production"
    },
    "hubVNetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "spokeVNetAddressPrefix": {
      "value": "10.1.0.0/16"
    },
    "azureFirewallSubnetPrefix": {
      "value": "10.0.1.0/24"
    },
    "gatewaySubnetPrefix": {
      "value": "10.0.2.0/24"
    },
    "workloadSubnetPrefix": {
      "value": "10.1.1.0/24"
    },
    "onPremisesAddressPrefix": {
      "value": "192.168.0.0/16"
    },
    "expressRouteGatewaySku": {
      "value": "Standard"
    },
    "logAnalyticsRetentionDays": {
      "value": 30
    },
    "enableFirewallPremiumFeatures": {
      "value": true
    },
    "threatIntelligenceMode": {
      "value": "Alert"
    },
    "idpsMode": {
      "value": "Alert"
    }
  }
}