{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "namePrefix": {
      "value": "compliance"
    },
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "tags": {
      "value": {
        "project": "compliance-reporting",
        "department": "security",
        "costCenter": "SEC-001",
        "owner": "ComplianceTeam",
        "criticality": "high",
        "dataClassification": "confidential"
      }
    },
    "enableHighAvailability": {
      "value": false
    },
    "logAnalyticsSkuName": {
      "value": "PerGB2018"
    },
    "logAnalyticsRetentionDays": {
      "value": 90
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "enableBlobVersioning": {
      "value": true
    },
    "workflowFrequency": {
      "value": "Hour"
    },
    "workflowInterval": {
      "value": 6
    },
    "notificationEmail": {
      "value": ""
    },
    "enableAdvancedThreatProtection": {
      "value": true
    }
  }
}