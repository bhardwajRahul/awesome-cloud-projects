{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "environment": {
      "value": "dev"
    },
    "projectName": {
      "value": "mariadb-migration"
    },
    "randomSuffix": {
      "value": "abc123"
    },
    "mysqlAdminLogin": {
      "value": "mysqladmin"
    },
    "mysqlAdminPassword": {
      "value": "MySQL123!SecurePassword"
    },
    "mysqlComputeTier": {
      "value": "GeneralPurpose"
    },
    "mysqlSkuName": {
      "value": "Standard_D2ds_v4"
    },
    "mysqlStorageSize": {
      "value": 128
    },
    "mysqlVersion": {
      "value": "5.7"
    },
    "enableHighAvailability": {
      "value": true
    },
    "highAvailabilityMode": {
      "value": "ZoneRedundant"
    },
    "backupRetentionDays": {
      "value": 35
    },
    "enableGeoRedundantBackup": {
      "value": true
    },
    "enableDiagnostics": {
      "value": true
    },
    "logAnalyticsRetentionDays": {
      "value": 90
    },
    "enablePrivateEndpoint": {
      "value": false
    },
    "vnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "databaseSubnetPrefix": {
      "value": "10.0.1.0/24"
    },
    "privateEndpointSubnetPrefix": {
      "value": "10.0.2.0/24"
    },
    "tags": {
      "value": {
        "Environment": "Development",
        "Project": "MariaDB Migration",
        "Purpose": "MariaDB to MySQL Migration",
        "CreatedBy": "Bicep Template",
        "Owner": "Database Team",
        "CostCenter": "IT-DB-001",
        "Criticality": "High",
        "MaintenanceWindow": "Saturday 02:00-06:00 UTC"
      }
    }
  }
}