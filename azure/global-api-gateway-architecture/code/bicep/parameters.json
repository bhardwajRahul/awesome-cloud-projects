{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "primaryRegion": {
      "value": "eastus"
    },
    "secondaryRegions": {
      "value": [
        "westeurope",
        "southeastasia"
      ]
    },
    "resourcePrefix": {
      "value": "contoso-api"
    },
    "environment": {
      "value": "production"
    },
    "publisherName": {
      "value": "Contoso API Platform"
    },
    "publisherEmail": {
      "value": "api-team@contoso.com"
    },
    "apimSku": {
      "value": "Premium"
    },
    "apimCapacity": {
      "value": 1
    },
    "cosmosConsistencyLevel": {
      "value": "Session"
    },
    "rateLimitThroughput": {
      "value": 10000
    },
    "enableZoneRedundancy": {
      "value": true
    },
    "logRetentionDays": {
      "value": 30
    }
  }
}