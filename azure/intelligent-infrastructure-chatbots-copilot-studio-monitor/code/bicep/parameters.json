{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "uniqueSuffix": {
      "value": "[uniqueString(resourceGroup().id)]"
    },
    "environmentName": {
      "value": "dev"
    },
    "projectName": {
      "value": "infra-bot"
    },
    "logAnalyticsSkuName": {
      "value": "PerGB2018"
    },
    "appInsightsSamplingPercentage": {
      "value": 100
    },
    "functionAppRuntime": {
      "value": "python"
    },
    "functionAppRuntimeVersion": {
      "value": "3.9"
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "resourceTags": {
      "value": {
        "Purpose": "infrastructure-chatbot",
        "Environment": "dev",
        "Project": "infra-bot",
        "Owner": "IT-Operations",
        "CostCenter": "Technology",
        "CreatedBy": "Azure-Bicep",
        "CreatedDate": "[utcNow('yyyy-MM-dd')]"
      }
    }
  }
}