{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "projectName": {
      "value": "healthmon"
    },
    "tags": {
      "value": {
        "environment": "development",
        "project": "infrastructure-health-monitoring",
        "purpose": "automated-infrastructure-monitoring",
        "deployedBy": "bicep",
        "costCenter": "IT-Operations",
        "owner": "infrastructure-team",
        "criticality": "medium"
      }
    },
    "vnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "functionsSubnetPrefix": {
      "value": "10.0.1.0/24"
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "functionRuntime": {
      "value": "python"
    },
    "functionRuntimeVersion": {
      "value": "3.11"
    },
    "logAnalyticsSku": {
      "value": "PerGB2018"
    },
    "enableApplicationInsights": {
      "value": true
    },
    "additionalKeyVaultAccessPolicies": {
      "value": [
        {
          "tenantId": "00000000-0000-0000-0000-000000000000",
          "objectId": "00000000-0000-0000-0000-000000000000",
          "permissions": {
            "secrets": ["get", "list", "set"],
            "keys": ["get", "list"],
            "certificates": ["get", "list"]
          }
        }
      ]
    }
  }
}