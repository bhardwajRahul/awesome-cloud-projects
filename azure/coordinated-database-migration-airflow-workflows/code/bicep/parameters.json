{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "uniqueSuffix": {
      "value": "$(uniqueString(resourceGroup().id))"
    },
    "tags": {
      "value": {
        "purpose": "database-migration",
        "environment": "dev",
        "recipe": "intelligent-migration-orchestration",
        "costCenter": "IT",
        "project": "database-modernization",
        "owner": "migration-team"
      }
    },
    "dataFactoryName": {
      "value": "adf-migration-$(uniqueString(resourceGroup().id))"
    },
    "enableManagedVNet": {
      "value": true
    },
    "enableGitIntegration": {
      "value": false
    },
    "gitConfiguration": {
      "value": {}
    },
    "dmsName": {
      "value": "dms-migration-$(uniqueString(resourceGroup().id))"
    },
    "dmsSku": {
      "value": "Standard_4vCores"
    },
    "storageAccountName": {
      "value": "st$(replace(uniqueString(resourceGroup().id), '-', ''))migration"
    },
    "storageAccountSku": {
      "value": "Standard_LRS"
    },
    "logAnalyticsName": {
      "value": "law-migration-$(uniqueString(resourceGroup().id))"
    },
    "logAnalyticsSku": {
      "value": "PerGB2018"
    },
    "logAnalyticsRetentionDays": {
      "value": 30
    },
    "enableAlerts": {
      "value": true
    },
    "alertNotificationEmail": {
      "value": "admin@company.com"
    },
    "actionGroupName": {
      "value": "ag-migration-$(uniqueString(resourceGroup().id))"
    },
    "enablePrivateEndpoints": {
      "value": false
    },
    "vnetResourceId": {
      "value": ""
    },
    "subnetResourceId": {
      "value": ""
    }
  }
}