{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environment": {
      "value": "dev"
    },
    "uniqueSuffix": {
      "value": "demo01"
    },
    "appNamePrefix": {
      "value": "content-moderation"
    },
    "contentSafetyTier": {
      "value": "S0"
    },
    "serviceBusTier": {
      "value": "Standard"
    },
    "storageAccountTier": {
      "value": "Standard_LRS"
    },
    "enableWorkloadProfiles": {
      "value": true
    },
    "containerCpu": {
      "value": "0.5"
    },
    "containerMemory": {
      "value": "1.0Gi"
    },
    "containerParallelism": {
      "value": 3
    },
    "containerReplicaTimeout": {
      "value": 300
    },
    "containerRetryLimit": {
      "value": 3
    },
    "scalingMessageCount": {
      "value": 5
    },
    "queueMaxSizeInMB": {
      "value": 5120
    },
    "messageTimeToLive": {
      "value": "PT24H"
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Application": "content-moderation",
        "Purpose": "Content Moderation Demo",
        "ManagedBy": "Bicep",
        "Owner": "DevOps Team",
        "CostCenter": "Engineering",
        "Project": "Content Safety Initiative"
      }
    }
  }
}