{
  "name": "iot-data-visualization-quicksight-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript application for IoT Data Visualization with QuickSight",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.4.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.133.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "iot",
    "quicksight",
    "kinesis",
    "glue",
    "analytics",
    "data-visualization"
  ],
  "author": "AWS CDK Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git"
  },
  "bugs": {
    "url": "https://github.com/aws/aws-cdk/issues"
  },
  "homepage": "https://github.com/aws/aws-cdk",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/cdk.out/**"
    ]
  }
}