{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.pytest_cache",
      "**/.coverage",
      "**/.venv",
      "**/.env",
      "**/node_modules"
    ]
  },
  "context": {
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": false,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-opensearchservice:enableOpensearchMultiAzWithStandby": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-lambda:logRetentionRetryOptions": true,
    "@aws-cdk/aws-dynamodb:resourcePolicyPerReplica": true,
    "@aws-cdk/aws-logs:logRetentionDetermineRoleRegion": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableEmrServicePolicyV2": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorMultipleBodyOrParameterModels": true,
    "@aws-cdk/aws-stepfunctions:includeExecutionDataInSnapshot": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-lambda:useAlarmNameForName": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": false,
    "@aws-cdk/aws-ecs:reduceRunTaskPermissions": true,
    "@aws-cdk/aws-lambda:recognizeVersionPropsWithDefaultRetriesAsync": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/aws-lambda:omitDeprecatedRuntimeFamilyChecks": true,
    "@aws-cdk/aws-stepfunctions-tasks:sfnTasksVpcEndpointV2": true,
    "@aws-cdk/aws-lambda:enableLoggingInformation": true,
    "@aws-cdk/aws-lambda:configureDefaultLogRetention": true,
    "@aws-cdk/aws-lambda:logGroupNamePrefix": true,
    "@aws-cdk/aws-lambda:enableLiveLambdaInsights": true,
    "@aws-cdk/aws-lambda:enableHighThroughputMode": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:attributeDefaultVpc": true,
    "@aws-cdk/aws-lambda:useCurrentRuntimeForInlineCode": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "resource_suffix": "dev"
  },
  "plugin": [
    "@aws-cdk/aws-lambda-python-alpha"
  ],
  "build": {
    "commands": [
      "python -m pip install --upgrade pip",
      "python -m pip install -r requirements.txt"
    ]
  },
  "diff": {
    "include": [
      "**"
    ],
    "exclude": [
      "cdk.out/**",
      "**/__pycache__/**",
      "**/*.pyc",
      ".pytest_cache/**",
      ".coverage",
      ".venv/**",
      ".env"
    ]
  },
  "deploy": {
    "parameters": {
      "resource_suffix": "prod"
    },
    "require-approval": "never",
    "ci": true
  },
  "destroy": {
    "ci": true
  },
  "feature_flags": {
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": false
  },
  "versionReporting": false,
  "pathMetadata": false,
  "assetMetadata": false,
  "notices": false,
  "lookups": false,
  "bundlingStacks": [],
  "rollback": false,
  "tags": {
    "Project": "WebhookProcessingSystem",
    "Environment": "Development",
    "Owner": "DevOps",
    "CostCenter": "Engineering",
    "Application": "WebhookProcessor"
  }
}