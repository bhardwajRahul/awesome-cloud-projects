{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "setup.py",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.venv",
      "**/.env",
      "**/node_modules",
      "**/cdk.out",
      "**/.git",
      "**/.pytest_cache",
      "**/.mypy_cache",
      "**/htmlcov",
      "**/*.egg-info",
      "**/dist",
      "**/build"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableLogging": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSizeCheck": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-normandy:enableLogging": true,
    "@aws-cdk/aws-lambda:codeconfigReportBatchingEnabled": true,
    "@aws-cdk/aws-apigateway:defaultMethodOptions": true,
    "@aws-cdk/aws-s3:enforceSSLRequestsOnlyOnSecureBucket": true,
    "@aws-cdk/aws-events:eventsTargetQueueSizeCheck": true,
    "@aws-cdk/aws-events:eventsTargetQueueSizeCheck": true,
    "@aws-cdk/aws-s3:useS3ExpressSemantics": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:reduceCrossAccountPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ec2:disableSubnetCreateDefaultRouteToIgw": true,
    "@aws-cdk/aws-ec2:ipv6Support": true,
    "@aws-cdk/aws-ec2:subnetIpv6NatGatewaySupport": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-ec2:unrestrictedSecurityGroupRules": true,
    "@aws-cdk/aws-lambda:logRetentionAddDefaultsToAwsAccountRegion": true,
    "@aws-cdk/aws-ec2:machineImageCallerOwnedAccountId": true,
    "@aws-cdk/aws-lambda:allowReservedConcurrentExecutions": true,
    "@aws-cdk/aws-rds:setCorrectValueForDatabaseInstanceReadReplicaInstanceResourceId": true,
    "@aws-cdk/aws-route53resolver:avoidDuplicateResourceCreation": true,
    "@aws-cdk/aws-ec2:enableDnsHostnamesForVpc": true,
    "@aws-cdk/aws-lambda:defineRuntime": true,
    "@aws-cdk/aws-lambda:checkForUpdatedRuntime": true,
    "@aws-cdk/aws-codeguru-reviewer:useByDefaultForCodeCommit": true,
    "@aws-cdk/aws-dynamodb:tableResourcePolicyAdded": true,
    "@aws-cdk/aws-iam:principalWriteActionsToSeparateStatements": true,
    "@aws-cdk/aws-ec2:imageId": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3Uri": true,
    "@aws-cdk/aws-cloudformation:checkForUpdatedExistingResource": true,
    "@aws-cdk/aws-logs:logRetentionUseServiceToken": true,
    "@aws-cdk/aws-ec2:natGatewayDefaultConnectivityType": true,
    "@aws-cdk/aws-ec2:configureNatGatewayDefaultSnatIPv6": true,
    "@aws-cdk/aws-stepfunctions-tasks:arnFormatIncludesRegion": true,
    "@aws-cdk/aws-lambda:useCurrentRuntimeForNewFunction": true,
    "@aws-cdk/aws-ec2:enablePartitionLiterals": true,
    "@aws-cdk/stack-tags:includeLowerCaseTags": true,
    "@aws-cdk/aws-logs:createLogRetentionLogGroup": true,
    "@aws-cdk/aws-lambda:useCurrentRuntimeForNewFunction": true,
    "@aws-cdk/aws-codebuild:useBuildEnvironmentComputeType": true,
    "aws:cdk:enable-path-metadata": true,
    "aws:cdk:enable-diff-no-fail": true,
    "aws:cdk:version-reporting": true,
    "aws:cdk:enable-stack-name-duplicates": true,
    "aws:cdk:enable-resource-property-constraint": true,
    "aws:cdk:dockerfile-ignore-patterns": [
      "**/.dockerignore",
      "**/*.md",
      "**/*.txt",
      "**/*.json",
      "**/*.js",
      "**/*.ts",
      "**/*.py",
      "**/*.java",
      "**/*.go",
      "**/*.rb",
      "**/*.php",
      "**/*.cs",
      "**/*.sh",
      "**/*.bat",
      "**/*.ps1",
      "**/*.yaml",
      "**/*.yml",
      "**/*.xml",
      "**/*.html",
      "**/*.css"
    ]
  },
  "toolkitStackName": "NetworkMicroSegmentationCDKToolkit",
  "toolkitBucket": {
    "bucketName": "network-microsegmentation-cdk-toolkit-${AWS::AccountId}-${AWS::Region}",
    "versioned": true,
    "lifecycleConfiguration": {
      "rules": [
        {
          "id": "DeleteOldVersions",
          "status": "Enabled",
          "noncurrentVersionExpiration": {
            "noncurrentDays": 30
          }
        }
      ]
    }
  },
  "requireApproval": "never",
  "lookups": false,
  "versionReporting": true,
  "pathMetadata": true,
  "analytics": false,
  "assetMetadata": true,
  "staging": true,
  "output": "cdk.out",
  "bundlingStacks": [],
  "notices": true,
  "profile": "default",
  "build": {
    "exclude": [
      "cdk.out",
      "**/__pycache__",
      "**/*.pyc",
      "**/.venv",
      "**/.env",
      "**/node_modules",
      "**/.git",
      "**/.pytest_cache",
      "**/.mypy_cache",
      "**/htmlcov",
      "**/*.egg-info",
      "**/dist",
      "**/build"
    ]
  },
  "deploy": {
    "confirmChanges": false,
    "requireApproval": "never"
  }
}