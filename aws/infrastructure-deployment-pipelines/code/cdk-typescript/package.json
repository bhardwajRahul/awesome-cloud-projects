{
  "name": "infrastructure-deployment-pipelines-cdk",
  "version": "0.1.0",
  "description": "Infrastructure deployment pipelines using AWS CDK and CodePipeline",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.136.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws/aws-cdk.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "codepipeline",
    "codecommit",
    "codebuild",
    "infrastructure",
    "deployment",
    "cicd"
  ],
  "author": "AWS CDK Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aws/aws-cdk/issues"
  },
  "homepage": "https://github.com/aws/aws-cdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  }
}