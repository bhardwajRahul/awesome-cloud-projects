{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".git/**",
      ".pytest_cache/**",
      "**/.mypy_cache/**",
      "**/__pycache__/**",
      "**/node_modules/**"
