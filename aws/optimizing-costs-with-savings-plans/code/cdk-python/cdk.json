{
  "app": "python app.py",
  "version": "1.0.0",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.venv",
      "**/.env",
      "**/node_modules",
      "**/.git",
      "**/*.zip",
      "**/*.tar.gz",
      "**/dist",
      "**/build",
      "**/*.egg-info",
      "**/.pytest_cache",
      "**/.coverage",
      "**/htmlcov",
      "**/.mypy_cache",
      "**/.DS_Store",
      "**/Thumbs.db"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-neptune:clusterParameterGroupRequiresDbClusterParameterGroupName": true,
    "@aws-cdk/aws-opensearchservice:opensearchDefaultLogPolicy": true,
    "@aws-cdk/aws-lambda-python:alwaysUseLatestPackageVersion": true,
    "aws-cdk:enableDiffNoFail": true,
    "@aws-cdk/aws-s3:enforceSSL": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-rds:singleUserRotationApplication": true,
    "@aws-cdk/aws-applicationautoscaling:generatePolicyNameForServiceRole": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:reduceCrossAccountRegionPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": true,
    "@aws-cdk/custom-resources:logRetentionSetRemovalPolicy": true,
    "@aws-cdk/aws-stepfunctions:stateMachineTypeIsStandardByDefault": true,
    "@aws-cdk/aws-logs:logRetentionSetRemovalPolicy": true,
    "@aws-cdk/aws-route53:deleteRoute53RecordSetUsingChangeBatch": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-codebuild:batchServiceRolePolicy": true,
    "@aws-cdk/aws-synthetics:enableAutoDeleteLambdas": true,
    "@aws-cdk/aws-apigateway:generateUniqueId": true,
    "@aws-cdk/aws-codedeploy:useOptimizedAssetPublishing": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-codebuild:batchServiceRolePolicy": true,
    "@aws-cdk/aws-synthetics:enableAutoDeleteLambdas": true,
    "@aws-cdk/aws-apigateway:generateUniqueId": true,
    "@aws-cdk/aws-codedeploy:useOptimizedAssetPublishing": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "lookups": [
    "account",
    "region",
    "availability-zones:account=${AWS::AccountId}:region=${AWS::Region}",
    "ssm:account=${AWS::AccountId}:parameterName=/aws/service/ami-amazon-linux-latest/amzn2-ami-hvm-x86_64-gp2:region=${AWS::Region}",
    "hosted-zone:account=${AWS::AccountId}:domainName=${domain}:region=${AWS::Region}",
    "vpc:account=${AWS::AccountId}:filter.group-id=${securityGroupId}:region=${AWS::Region}",
    "vpc:account=${AWS::AccountId}:filter.isDefault=true:region=${AWS::Region}",
    "key-provider:account=${AWS::AccountId}:aliasName=alias/aws/s3:region=${AWS::Region}",
    "vpc:account=${AWS::AccountId}:filter.tag:Name=${vpcName}:region=${AWS::Region}",
    "security-group:account=${AWS::AccountId}:groupId=${securityGroupId}:region=${AWS::Region}",
    "subnet:account=${AWS::AccountId}:filter.vpc-id=${vpcId}:region=${AWS::Region}",
    "subnet:account=${AWS::AccountId}:subnetId=${subnetId}:region=${AWS::Region}",
    "subnet:account=${AWS::AccountId}:filter.availability-zone=${availabilityZone}:filter.vpc-id=${vpcId}:region=${AWS::Region}",
    "load-balancer:account=${AWS::AccountId}:loadBalancerArn=${loadBalancerArn}:region=${AWS::Region}",
    "load-balancer-listener:account=${AWS::AccountId}:listenerArn=${listenerArn}:region=${AWS::Region}"
  ],
  "bundlingOptions": {
    "enabled": true,
    "assetHashType": "output"
  },
  "assetMetadata": true,
  "pathMetadata": true,
  "versionReporting": true,
  "analytics": false,
  "build": {
    "exclude": [
      "cdk.out",
      "node_modules",
      "*.pyc",
      "__pycache__",
      ".git",
      ".pytest_cache",
      ".coverage",
      "htmlcov",
      ".mypy_cache",
      "dist",
      "build",
      "*.egg-info"
    ]
  },
  "tags": {
    "Project": "SavingsPlansRecommendations",
    "Environment": "production",
    "Owner": "cost-optimization-team",
    "ManagedBy": "CDK"
  },
  "requireApproval": "never",
  "ci": true,
  "output": "cdk.out",
  "notices": true,
  "bootstrapQualifier": "hnb659fds",
  "hotswap": false,
  "diff": true,
  "rollback": true,
  "progress": "events",
  "staging": true,
  "assetMetadata": true,
  "pathMetadata": true,
  "versionReporting": true,
  "acknowledgements": {
    "aws-cdk-lib": "AWS CDK is licensed under the Apache 2.0 License",
    "constructs": "Constructs library is licensed under the Apache 2.0 License"
  }
}