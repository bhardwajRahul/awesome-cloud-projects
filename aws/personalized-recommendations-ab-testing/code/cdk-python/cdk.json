{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigatewayv2:httpApiDefaultAuthorizerToTheRoute": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeArchitectureProps": true,
    "@aws-cdk/aws-stepfunctions:enableLogging": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda-nodejs:esbuildArgs": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "stack_name": "PersonalizeRecommendationStack",
    "project_name": "personalize-ab-testing",
    "environment": "development",
    "enable_cloudwatch_insights": true,
    "enable_x_ray_tracing": true,
    "lambda_memory_sizes": {
      "ab_router": 256,
      "recommendation": 512,
      "event_tracker": 256,
      "personalize_manager": 512,
      "analytics": 512
    },
    "lambda_timeout_seconds": {
      "ab_router": 30,
      "recommendation": 30,
      "event_tracker": 30,
      "personalize_manager": 300,
      "analytics": 300
    },
    "dynamodb_billing_mode": "PAY_PER_REQUEST",
    "kinesis_shard_count": 2,
    "kinesis_retention_days": 7,
    "s3_lifecycle_rules": {
      "ia_transition_days": 30,
      "glacier_transition_days": 90
    },
    "cloudwatch_log_retention_days": 7,
    "api_gateway_throttling": {
      "rate_limit": 1000,
      "burst_limit": 2000
    },
    "personalize_config": {
      "min_provisioned_tps": 1,
      "auto_scaling_enabled": false,
      "training_timeout_hours": 6
    },
    "security_config": {
      "encryption_at_rest": true,
      "encryption_in_transit": true,
      "enable_vpc": false,
      "enable_waf": false
    },
    "monitoring_config": {
      "enable_detailed_monitoring": true,
      "create_dashboard": true,
      "enable_alarms": true
    },
    "ab_testing_config": {
      "default_split": {
        "variant_a": 0.33,
        "variant_b": 0.33,
        "variant_c": 0.34
      },
      "consistent_hashing": true
    }
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "pathMetadata": "@aws-cdk/core:path",
  "assetMetadata": "@aws-cdk/core:asset-path",
  "versionReporting": false,
  "analytics": false,
  "notices": true,
  "lookups": true,
  "bundlingStacks": [],
  "build": "python3 -m py_compile app.py",
  "tags": {
    "Project": "PersonalizeRecommendations",
    "Environment": "Development",
    "CreatedBy": "CDK",
    "CostCenter": "Engineering",
    "Application": "RealtimeRecommendations"
  },
  "requireApproval": "never",
  "diff": true
}