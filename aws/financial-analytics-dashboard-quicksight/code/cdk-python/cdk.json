{
  "app": "python app.py",
  "version_reporting": true,
  "analytics_reporting": false,
  "stack_traces": true,
  "notices": true,
  "require_approval": "broadening",
  "lookups": false,
  "feature_flags": {
    "aws-cdk-lib/aws-lambda:recognizeLayerVersion": true,
    "aws-cdk-lib/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-efs:mountTargetOrderInsensitiveLogicalId": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-codepipeline-actions:useNewDefaultBranchForSourceActions": true,
    "@aws-cdk/aws-s3-deployment:createWarningsForSkippedFiles": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingForEMRStepFunctions": true,
    "@aws-cdk/aws-ec2:instanceRequireImdsv2": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:reduceEc2FargateCloudWatchPermissions": true,
    "@aws-cdk/aws-route53resolver:migrateFromCfnIncludeToL2": true,
    "@aws-cdk/aws-events-targets:sqsQueueMessageId": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ]
  },
  "metadata": {
    "analytics_reporting": {
      "enabled": false,
      "version": "2"
    }
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "aws-cdk:enableDiffNoFail": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true
  },
  "build": {
    "exclude": [
      "cdk.out",
      ".git",
      "__pycache__",
      "*.pyc",
      ".pytest_cache",
      ".coverage",
      ".env",
      "node_modules",
      "*.egg-info",
      "dist",
      "build"
    ]
  },
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".pytest_cache/**",
      ".coverage",
      "cdk.out/**",
      ".git/**",
      "**/*.egg-info/**",
      "dist/**",
      "build/**"
    ]
  },
  "deployment": {
    "account_mapping": {
      "dev": {
        "account": "123456789012",
        "region": "us-east-1"
      },
      "staging": {
        "account": "123456789012", 
        "region": "us-east-1"
      },
      "prod": {
        "account": "123456789012",
        "region": "us-east-1"
      }
    }
  },
  "tags": {
    "Project": "FinancialAnalytics",
    "Environment": "Production",
    "Owner": "FinanceTeam",
    "CostCenter": "IT",
    "Application": "FinancialAnalyticsDashboard",
    "DataClassification": "Confidential"
  },
  "toolkit_stack_name": "CDKToolkit",
  "bootstrap": {
    "trust": ["123456789012"],
    "cloudformation_execution_policies": [
      "arn:aws:iam::aws:policy/AdministratorAccess"
    ]
  },
  "bundling": {
    "stacks": ["*"],
    "bundle_timeout": 600
  }
}