{
  "app": "python3 app.py",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/core:newStyleStackSynthesis": true,
    "@aws-cdk/core:bootstrapQualifier": "hnb659fds",
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-s3-deployment:retainOnDelete": false,
    "@aws-cdk/aws-cloudformation:checkSecretUsage": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-route53:splitHorizonZone": true,
    "@aws-cdk/aws-stepfunctions:enableLogging": true,
    "@aws-cdk/aws-glue:jobBookmarkParam": true,
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-cloudwatch:crossEnvironment": true,
    "@aws-cdk/aws-cloudtrail:enableFileValidation": true,
    "@aws-cdk/aws-lakeformation:enableDataLakeSettings": true,
    "aws:cdk:enable-path-metadata": true,
    "aws:cdk:enable-asset-metadata": true,
    "aws:cdk:version-reporting": true,
    "aws:cdk:enable-diff-no-fail": true,
    "aws:cdk:disable-type-check": false,
    "aws:cdk:bundling-stacks": [],
    "environment": "dev",
    "project": "data-catalog-governance",
    "dataClassification": {
      "enablePIIDetection": true,
      "customClassifiers": ["pii-classifier"],
      "sensitiveDataTypes": ["ssn", "email", "phone", "credit_card", "address"]
    },
    "governance": {
      "enableLakeFormation": true,
      "enableCloudTrail": true,
      "enableCloudWatch": true,
      "auditRetentionDays": 2557,
      "enableDataLineage": true
    },
    "monitoring": {
      "enableDashboard": true,
      "enableAlerts": true,
      "logRetentionDays": 365,
      "enableMetrics": true
    },
    "security": {
      "enableEncryption": true,
      "enableMFA": false,
      "enableCrossAccountAccess": false,
      "enableVPC": false
    },
    "cost": {
      "enableLifecyclePolicy": true,
      "enableIntelligentTiering": true,
      "enableSpotInstances": false
    }
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "requireApproval": "any-change",
  "assetMetadata": true,
  "pathMetadata": true,
  "versionReporting": true,
  "analyticsReporting": true,
  "bundlingStacks": [],
  "lookups": true,
  "build": {
    "exclude": [
      "**/__pycache__",
      "**/*.pyc",
      "**/*.pyo",
      "**/.pytest_cache",
      "**/.coverage",
      "**/htmlcov",
      "**/.tox",
      "**/.mypy_cache",
      "**/venv",
      "**/env",
      "**/.venv",
      "**/.env",
      "**/node_modules",
      "**/dist",
      "**/build",
      "**/.git",
      "**/.gitignore",
      "**/README.md",
      "**/.DS_Store",
      "**/*.md",
      "**/*.txt",
      "**/*.log"
    ]
  },
  "deploy": {
    "requireApproval": "any-change",
    "rollback": true,
    "hotswap": false,
    "watch": false,
    "parameters": {
      "environment": "dev",
      "region": "us-east-1",
      "enablePIIDetection": true,
      "enableAuditLogging": true,
      "enableMonitoring": true
    }
  },
  "destroy": {
    "requireApproval": "any-change",
    "force": false
  },
  "diff": {
    "requireApproval": "any-change",
    "fail": false
  },
  "metadata": {
    "aws:cdk:path": "DataCatalogGovernanceStack/metadata.json"
  },
  "tags": {
    "Project": "DataCatalogGovernance",
    "Environment": "dev",
    "ManagedBy": "CDK",
    "Purpose": "DataGovernance",
    "Compliance": "GDPR,HIPAA,SOX",
    "Owner": "DataEngineering",
    "CostCenter": "IT",
    "Version": "1.0.0"
  }
}