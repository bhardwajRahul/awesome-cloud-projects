    "@aws-cdk/aws-lambda:disableLogRetentionFeature": false,
    "@aws-cdk/aws-lambda:enableInsightsVersionProp": true,
    "@aws-cdk/aws-stepfunctions:enableLogging": true,
    "@aws-cdk/aws-s3:enforceSSL": true,
    "@aws-cdk/aws-s3:bucketGrantWriteWithoutAcl": true,
    "@aws-cdk/aws-s3:blockPublicReadAccess": true,
