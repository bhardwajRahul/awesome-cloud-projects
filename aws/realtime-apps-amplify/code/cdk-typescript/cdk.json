    "@aws-cdk/aws-redshift:defaultEnhancedVpcRouting": true,
    "@aws-cdk/aws-redshift:defaultPubliclyAccessible": false,
    "@aws-cdk/aws-redshift:defaultDeletionProtection": true,
    "@aws-cdk/aws-redshift:defaultBackupRetentionPeriod": true,
    "@aws-cdk/aws-redshift:defaultPreferredMaintenanceWindow": true,
    "@aws-cdk/aws-redshift:defaultAutomatedSnapshotRetentionPeriod": true,
    "@aws-cdk/aws-redshift:defaultManualSnapshotRetentionPeriod": true,
    "@aws-cdk/aws-redshift:defaultMasterUserPassword": true,
    "@aws-cdk/aws-redshift:defaultAvailabilityZone": true,
    "@aws-cdk/aws-redshift:defaultPort": true,
    "@aws-cdk/aws-redshift:defaultVpcSecurityGroupIds": true,
    "@aws-cdk/aws-redshift:defaultClusterVersion": true,
    "@aws-cdk/aws-redshift:defaultNodeType": true,
    "@aws-cdk/aws-redshift:defaultNumberOfNodes": true,
