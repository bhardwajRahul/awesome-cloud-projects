{
  "name": "s3-inventory-storage-analytics-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript application for S3 Inventory and Storage Analytics Reporting",
  "author": "AWS CDK",
  "license": "MIT",
  "bin": {
    "s3-inventory-storage-analytics-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.115.0",
    "constructs": "^10.3.0"
  },
  "main": "lib/app.js",
  "types": "lib/app.d.ts",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  },
  "keywords": [
    "aws",
    "cdk",
    "s3",
    "storage",
    "analytics",
    "inventory",
    "reporting",
    "athena",
    "cloudwatch",
    "lambda"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/s3-inventory-storage-analytics"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}