  "verbose": false,
  "debug": false,
  "trace": false,
  "strict": true,
  "execute": true,
  "force": false,
  "reuseAssets": [],
  "exclusively": false,
  "changeSetName": "",
  "rollback": true,
  "notificationArns": [],
