{
  "name": "scheduled-email-reports-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript application for scheduled email reports using App Runner and SES",
  "bin": {
    "scheduled-email-reports-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "email-reports",
    "app-runner",
    "ses",
    "eventbridge-scheduler",
    "cloudwatch"
  ],
  "author": "AWS CDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/scheduled-email-reports-cdk.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}