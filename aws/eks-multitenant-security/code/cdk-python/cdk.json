    "eks-multi-tenant-security": {
      "cluster-name": "multi-tenant-cluster",
      "kubernetes-version": "1.29",
      "tenant-a-name": "tenant-alpha",
      "tenant-b-name": "tenant-beta",
      "vpc-cidr": "10.0.0.0/16",
      "enable-logging": true,
      "enable-network-policies": true,
      "enable-resource-quotas": true,
      "node-instance-type": "t3.medium",
