      "node-max-size": 5,
      "node-desired-size": 3,
      "enable-private-endpoint": true,
      "enable-public-endpoint": true,
      "log-retention-days": 7,
      "cluster-endpoint-access": "PRIVATE_AND_PUBLIC",
      "authentication-mode": "API_AND_CONFIG_MAP",
      "capacity-type": "ON_DEMAND",
      "ami-type": "AL2_X86_64",
      "disk-size": 20,
      "enable-cluster-autoscaler": true,
      "enable-aws-load-balancer-controller": true,
      "enable-ebs-csi-driver": true,
