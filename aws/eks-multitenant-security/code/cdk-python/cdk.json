        "authenticator": true,
        "controllerManager": true,
        "scheduler": false,
        "audit": false
      },
      "resource-quotas": {
        "requests-cpu": "2",
        "requests-memory": "4Gi",
        "limits-cpu": "4",
        "limits-memory": "8Gi",
        "pods": "10",
        "services": "5",
        "secrets": "10",
        "configmaps": "10",
        "persistentvolumeclaims": "4"
      },
      "limit-ranges": {
        "default-cpu": "200m",
        "default-memory": "256Mi",
        "default-request-cpu": "100m",
        "default-request-memory": "128Mi"
      },
      "sample-applications": {
        "tenant-alpha": {
          "image": "nginx:1.20",
          "port": 80,
          "replicas": 2
        },
        "tenant-beta": {
          "image": "httpd:2.4",
          "port": 80,
          "replicas": 2
        }
      }
    }
