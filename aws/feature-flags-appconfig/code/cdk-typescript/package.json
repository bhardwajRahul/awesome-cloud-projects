{
  "name": "feature-flags-appconfig-cdk",
  "version": "1.0.0",
  "description": "CDK TypeScript application for Feature Flags with AWS AppConfig",
  "bin": {
    "feature-flags-appconfig-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2",
    "aws-cdk": "2.100.0",
    "cdk-nag": "^2.27.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "@aws-solutions-constructs/aws-lambda-dynamodb": "^2.47.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "lib/**/*.ts",
      "!lib/**/*.d.ts"
    ]
  },
  "keywords": [
    "aws",
    "cdk",
    "appconfig",
    "feature-flags",
    "lambda",
    "cloudwatch"
  ],
  "author": "AWS CDK Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git"
  }
}