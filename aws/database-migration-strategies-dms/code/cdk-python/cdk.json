{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".git/**",
      "node_modules/**"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target": "aws-cdk-lib",
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": false,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "aws-cdk:enableDiffNoFail": true,
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingForLambdaInvoke": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-neptune:defaultAssociatedRole": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudformation:parseInterfaceVpcEndpointAwsDomainSuffix": true,
    "@aws-cdk/aws-logs:removeRetentionPolicyResourcePolicy": true,
    "@aws-cdk/aws-route53-resolver:alwaysCreateDnsFirewallDomainListId": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-resolver:removeEmptyDnsFirewallRuleProperty": true,
    "@aws-cdk/aws-ecr:defaultTagImmutability": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "source_db_config": {
      "engine_name": "mysql",
      "server_name": "source-database.example.com",
      "port": 3306,
      "database_name": "production_db",
      "username": "migration_user",
      "password": "secure_password_123",
      "extra_connection_attributes": "initstmt=SET foreign_key_checks=0;connectTimeout=60000"
    },
    "target_db_config": {
      "engine_name": "mysql",
      "server_name": "target-rds.us-east-1.rds.amazonaws.com",
      "port": 3306,
      "database_name": "migrated_db",
      "username": "admin",
      "password": "target_password_456",
      "extra_connection_attributes": "initstmt=SET foreign_key_checks=0;connectTimeout=60000"
    },
    "replication_instance_class": "dms.t3.medium",
    "enable_multi_az": true,
    "allocated_storage": 100,
    "vpc_cidr": "10.0.0.0/16"
  },
  "toolkit": {
    "stackTags": {
      "Environment": "migration",
      "Project": "database-migration-dms",
      "ManagedBy": "aws-cdk",
      "Owner": "data-team"
    }
  },
  "tags": {
    "Environment": "migration",
    "Project": "database-migration-dms",
    "ManagedBy": "aws-cdk",
    "CostCenter": "data-engineering",
    "Purpose": "database-migration"
  },
  "build": {
    "exclude": [
      "**/__pycache__/**",
      "**/*.pyc",
      ".git/**",
      ".venv/**",
      "node_modules/**",
      "*.log"
    ]
  },
  "requireApproval": "never",
  "diff": {
    "contextLines": 3
  },
  "lookups": {
    "account": true,
    "region": true,
    "availabilityZones": true
  },
  "versioning": false,
  "pathMetadata": true,
  "assetMetadata": true,
  "output": "cdk.out",
  "notices": true,
  "bundlingStacks": [],
  "hotswap": false,
  "profile": "",
  "rollback": true,
  "staging": true,
  "versionReporting": true
}