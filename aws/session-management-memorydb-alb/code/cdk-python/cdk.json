{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.pytest_cache/**",
      "**/.coverage",
      "**/.venv/**",
      "**/.env/**",
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/*.egg-info/**"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingConfiguration": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudformation:parseTemplateParameterTags": true,
    "@aws-cdk/aws-elasticloadbalancingv2:enableGrpcHealthCheck": true,
    "@aws-cdk/aws-batch:submitJobQueueDefinition": true,
    "aws-cdk:enableDiffNoFail": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr:defaultTagImmutability": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:reduceRunTaskPermissions": true,
    "@aws-cdk/aws-lambda:recognizeRuntimeFamily": true
  },
  "profile": "default",
  "toolkitStackName": "CDKToolkit",
  "pathMetadata": false,
  "assetMetadata": false,
  "versionReporting": false,
  "notices": true,
  "bundlingStacks": [],
  "lookups": true,
  "rollback": true,
  "stackTags": {
    "Project": "DistributedSessionManagement",
    "ManagedBy": "AWS-CDK",
    "Recipe": "distributed-session-management-memorydb-application-load-balancer"
  },
  "tags": {
    "Environment": "dev",
    "CostCenter": "Infrastructure",
    "Owner": "Platform-Team"
  },
  "requireApproval": "never",
  "hotswap": false,
  "hotswapFallback": true,
  "output": "cdk.out",
  "parameters": {
    "Environment": "dev",
    "InstanceType": "db.r6g.large",
    "MinCapacity": "2",
    "MaxCapacity": "10",
    "DesiredCapacity": "2"
  },
  "build": {
    "commands": [
      "echo 'Building CDK application...'",
      "python -m py_compile app.py",
      "echo 'Build completed successfully!'"
    ]
  },
  "test": {
    "commands": [
      "echo 'Running tests...'",
      "python -m pytest tests/ -v --cov=. --cov-report=term-missing",
      "echo 'Tests completed successfully!'"
    ]
  },
  "deploy": {
    "requireApproval": "never",
    "parameters": {
      "BootstrapVersion": "/cdk-bootstrap/hnb659fds/version"
    },
    "reuseAssets": [
      "*"
    ]
  },
  "destroy": {
    "force": false
  },
  "diff": {
    "contextLines": 3,
    "template": false
  },
  "metadata": {
    "analyticsReporting": true,
    "stackTraces": true
  },
  "featureFlags": {
    "@aws-cdk/core:newStyleStackSynthesis": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-ecr:defaultTagImmutability": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeRuntimeFamily": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-ecs:reduceRunTaskPermissions": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true
  },
  "environments": {
    "dev": {
      "account": "${CDK_DEFAULT_ACCOUNT}",
      "region": "${CDK_DEFAULT_REGION}",
      "parameters": {
        "Environment": "dev",
        "InstanceType": "db.r6g.large",
        "MinCapacity": "2",
        "MaxCapacity": "10"
      }
    },
    "staging": {
      "account": "${CDK_STAGING_ACCOUNT}",
      "region": "${CDK_STAGING_REGION}",
      "parameters": {
        "Environment": "staging",
        "InstanceType": "db.r6g.xlarge",
        "MinCapacity": "3",
        "MaxCapacity": "15"
      }
    },
    "prod": {
      "account": "${CDK_PROD_ACCOUNT}",
      "region": "${CDK_PROD_REGION}",
      "parameters": {
        "Environment": "prod",
        "InstanceType": "db.r6g.2xlarge",
        "MinCapacity": "5",
        "MaxCapacity": "25"
      }
    }
  },
  "bootstrapQualifier": "hnb659fds",
  "customCommands": {
    "bootstrap": "cdk bootstrap --toolkit-stack-name CDKToolkit-${Environment}",
    "deploy-dev": "cdk deploy --context env=dev --require-approval never",
    "deploy-staging": "cdk deploy --context env=staging --require-approval broadening",
    "deploy-prod": "cdk deploy --context env=prod --require-approval broadening",
    "destroy-dev": "cdk destroy --context env=dev --force",
    "diff-dev": "cdk diff --context env=dev",
    "synth-dev": "cdk synth --context env=dev",
    "ls": "cdk ls",
    "metadata": "cdk metadata",
    "docs": "cdk docs"
  },
  "logging": {
    "level": "INFO"
  },
  "validation": {
    "enabled": true,
    "policies": [
      "security-check",
      "best-practices",
      "cost-optimization"
    ]
  },
  "monitoring": {
    "enabled": true,
    "dashboards": true,
    "alarms": true,
    "metrics": true
  },
  "security": {
    "enableResourceExplorer": true,
    "enableConfigRules": true,
    "enableCloudTrail": true,
    "encryptionAtRest": true,
    "encryptionInTransit": true
  }
}