{
  "name": "iot-rules-engine-event-processing",
  "version": "1.0.0",
  "description": "CDK TypeScript implementation for IoT Rules Engine Event Processing",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
