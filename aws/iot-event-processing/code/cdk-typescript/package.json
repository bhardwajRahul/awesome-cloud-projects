    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap"
  },
  "keywords": [
    "aws",
    "cdk",
    "iot",
    "rules-engine",
    "event-processing",
    "lambda",
    "dynamodb",
    "sns"
  ],
  "author": "AWS CDK",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/aws-cdk-examples.git"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/*.(test|spec).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}