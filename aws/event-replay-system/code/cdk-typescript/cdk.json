{
  "app": "npx ts-node --prefer-ts-exts app.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-norththamesstudio:disableDefaultLogStreamPrefix": true,
    "@aws-cdk/aws-lambda:codeBucketPrefix": true,
    "@aws-cdk/aws-route53resolver:RouteTable": true,
    "@aws-cdk/aws-route53resolver:FirewallRuleGroupAssociation": true,
    "@aws-cdk/aws-ecr:defaultTagImmutability": true,
    "@aws-cdk/aws-globalaccelerator:acceleratorSecurityGroupsFromDifferentStack": true,
    "@aws-cdk/aws-s3:bucket-notification-ensure-cross-region-permissions": true,
    "@aws-cdk/aws-ecs:reduceRunTaskPermissions": true,
    "@aws-cdk/aws-events:crossAccountManagerPolicy": true,
    "@aws-cdk/aws-sqs:fifoQueueHighThroughput": true,
    "@aws-cdk/aws-synthetics:enableAutoDeleteLambdas": true,
    "@aws-cdk/aws-apigateway:generateUniqueCloudFormationName": true,
    "@aws-cdk/aws-logs:logGroupMetricFilterName": true,
    "@aws-cdk/aws-logs:logGroupMultilineQuery": true,
    "@aws-cdk/aws-stepfunctions:stateMachineArn": true,
    "@aws-cdk/aws-logs:logGroupCreateLogStream": true,
    "@aws-cdk/aws-s3:bucketGrantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs:limitSecretsManagerSecretAccess": true,
    "@aws-cdk/aws-secretsmanager:disableSecretDefaultPolicy": true,
    "@aws-cdk/aws-codepipeline:introducedCrossStackOutputs": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": true,
    "@aws-cdk/aws-ecs:taskDefinitionDefaultTaskRole": true,
    "@aws-cdk/aws-lambda:recognizePlatformVersionProps": true,
    "@aws-cdk/aws-events:generatedRuleName": true,
    "@aws-cdk/aws-events:crossAccountRuleName": true,
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/aws-s3:bucketNotificationsHandlerRoleArn": true,
    "@aws-cdk/aws-logs:logGroupVpcDefaultLogPolicy": true,
    "@aws-cdk/aws-logs:logGroupDisableCreationLogPolicy": true,
    "@aws-cdk/aws-s3-deployment:createDefaultLogRetentionPolicy": true,
    "@aws-cdk/aws-logs:logGroupResourcePolicy": true,
    "@aws-cdk/aws-codebuild:privilegedModeVariant": true,
    "@aws-cdk/aws-logs:logGroupResourcePolicyName": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:separateEcsRestartPolicyNamespace": true,
    "@aws-cdk/aws-efs:mountTargetOrderInsensitiveLogicalId": true,
    "@aws-cdk/aws-s3:bucketWebsiteUseGlobalEndpoint": true,
    "@aws-cdk/aws-s3:bucketWebsitePublicAccess": true,
    "@aws-cdk/aws-s3:crossRegionReplicationKmsKeyGeneratedByDefault": true,
    "@aws-cdk/aws-s3-deployment:keepExistingObjectsInDestinationBucket": true,
    "@aws-cdk/aws-s3:bucketNotificationProcessorRoleArn": true,
    "@aws-cdk/aws-logs:logGroupRetentionAddACustomResourceAndMethodDeletionPolicy": true,
    "@aws-cdk/aws-events:eventsHandlerRoleArn": true
  },
  "requireApproval": "never",
  "bootstrapStackVersionSsm": "/cdk-bootstrap/hnb659fds/version",
  "lookups": false,
  "featureFlags": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true
  },
  "build": {
    "compile": "npx tsc",
    "test": "npx jest"
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "version": "2.100.0"
}