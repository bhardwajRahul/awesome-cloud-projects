{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".git/**",
      ".pytest_cache/**",
      "**/.mypy_cache/**",
      "**/node_modules/**"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableLogging": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-eks:alb-controller-version": "v2.4.1",
    "cluster_name": "ack-operators-cluster",
    "ack_namespace": "ack-system",
    "region": "us-east-1",
    "account": "",
    "project_name": "kubernetes-operators",
    "environment": "demo",
    "enable_logging": true,
    "enable_monitoring": true,
    "node_instance_type": "t3.medium",
    "min_nodes": 2,
    "max_nodes": 4,
    "desired_nodes": 2,
    "kubernetes_version": "1.28",
    "vpc_cidr": "10.0.0.0/16",
    "enable_nat_gateway": true,
    "single_nat_gateway": true,
    "enable_dns_hostnames": true,
    "enable_dns_support": true,
    "s3_bucket_versioning": true,
    "s3_lifecycle_enabled": true,
    "lambda_runtime": "python3.9",
    "lambda_timeout": 60,
    "lambda_memory_size": 128,
    "log_retention_days": 7,
    "enable_cloudwatch_logs": true,
    "enable_vpc_flow_logs": false,
    "enable_guardduty": false,
    "enable_config": false,
    "enable_cloudtrail": false,
    "cost_allocation_tags": {
      "Project": "KubernetesOperators",
      "Environment": "Demo",
      "ManagedBy": "CDK",
      "Recipe": "kubernetes-operators-aws-resources",
      "Owner": "Platform Team",
      "CostCenter": "Engineering"
    },
    "security_groups": {
      "allow_ssh": false,
      "allow_http": false,
      "allow_https": true,
      "restrict_access": true
    },
    "backup_configuration": {
      "enable_s3_versioning": true,
      "s3_lifecycle_rules": true,
      "lambda_log_retention": 7
    },
    "monitoring_configuration": {
      "enable_cloudwatch": true,
      "enable_xray": false,
      "detailed_monitoring": false
    },
    "eks_configuration": {
      "enable_irsa": true,
      "enable_cluster_logging": true,
      "enable_private_endpoint": true,
      "enable_public_endpoint": true,
      "endpoint_private_access": true,
      "endpoint_public_access": true,
      "public_access_cidrs": ["0.0.0.0/0"],
      "cluster_encryption": true,
      "node_group_capacity_type": "ON_DEMAND",
      "node_group_ami_type": "AL2_x86_64",
      "node_group_disk_size": 20
    },
    "ack_configuration": {
      "s3_controller_enabled": true,
      "iam_controller_enabled": true,
      "lambda_controller_enabled": true,
      "custom_operator_enabled": true,
      "webhook_enabled": true,
      "monitoring_enabled": true
    },
    "feature_flags": {
      "enable_advanced_networking": false,
      "enable_service_mesh": false,
      "enable_gitops": false,
      "enable_policy_enforcement": true,
      "enable_cost_optimization": false
    }
  },
  "toolkits": [
    {
      "name": "default",
      "bootstrap": {
        "bucketName": "cdk-bootstrap-hnb659fds-${AWS::AccountId}-${AWS::Region}",
        "fileAssetsBucketName": "cdk-bootstrap-hnb659fds-${AWS::AccountId}-${AWS::Region}",
        "qualifier": "hnb659fds"
      }
    }
  ],
  "versionReporting": false,
  "pathMetadata": false,
  "assetMetadata": false,
  "staging": true,
  "output": "cdk.out",
  "lookups": true,
  "bundling": {
    "stacks": ["*"]
  },
  "notices": true,
  "cli": {
    "version-reporting": false,
    "notices": true,
    "path-metadata": false,
    "asset-metadata": false,
    "analytics": false
  }
}