{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".venv/**",
      ".env",
      "**/.git/**",
      "**/.pytest_cache/**",
      "**/node_modules/**",
      "**/*.egg-info/**"
    ]
  },
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/aws-cloudformation:parseIntegerArrayToJson": true,
    "@aws-cdk/aws-events:addEventsToRolePolicy": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2019": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/aws-logs:logRetentionUseArnOnOverlappingResource": true,
    "@aws-cdk/aws-route53:deleteRoute53RecordSet": true,
    "@aws-cdk/aws-route53resolver:deleteRoute53ResolverRule": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-codepipeline-actions:useNewDefaultBranchForCodeCommitSource": true,
    "@aws-cdk/aws-s3-notifications:authorizationPropertiesRestrictedByIncomingRules": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingConfiguration": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:reduceCrossAccountKeyPolicySize": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": false,
    "@aws-cdk/aws-lambda:functionNameWithUrlDefaultGrant": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": false,
    "@aws-cdk/aws-ecs:reduceRunTaskPermissions": true,
    "@aws-cdk/aws-eks:nodeGroupRemoteAccess": true,
    "@aws-cdk/aws-s3:autoDeleteObjectsForBuckets": true,
    "@aws-cdk/aws-ec2:ipv6AddressOnCreation": false,
    "@aws-cdk/aws-lambda:checkSecurityGroupRules": true,
    "@aws-cdk/aws-iam:principalWriteConditions": true,
    "@aws-cdk/aws-ecs:disableTaskDefinitionLogCreation": false,
    "@aws-cdk/aws-s3-deployment:useEfsInsteadOfEbs": false,
    "@aws-cdk/aws-ecs:enableExecuteCommand": true,
    "@aws-cdk/aws-lambda:useCurrentVersionForBucketDeployment": true,
    "@aws-cdk/aws-ecs:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-ec2:natGatewayProvisionedByDefault": true,
    "@aws-cdk/aws-stepfunctions:includeExecutionDataInStateDefinition": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:allowNonUniqueAdditionalTargets": false,
    "@aws-cdk/aws-lambda:defineRuntime": true,
    "@aws-cdk/aws-lambda:functionNameWithUrlDefaultGrant": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:reduceCrossAccountKeyPolicySize": true,
    "@aws-cdk/aws-eks:clusterLoggingCloudWatchLogGroups": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:blockDeviceMappingEnabled": true,
    "@aws-cdk/aws-lambda:defineRuntime": true,
    "@aws-cdk/aws-events:targetPartitionIdAsString": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3Uri": false,
    "@aws-cdk/aws-iot:createPolicyDocumentWithJsonTemplate": true,
    "@aws-cdk/aws-stepfunctions-tasks:enablePartitionedPrefix": false,
    "@aws-cdk/aws-glue:useConnectionAttributeAsConnectionName": true,
    "@aws-cdk/aws-appflow:useCurrentConnectorVersion": true
  },
  "toolkitStackName": "CDKToolkit",
  "toolkitBucket": {
    "bucketName": "cdk-toolkit-bucket",
    "versioned": true
  },
  "plugins": [
    "cdk-assets"
  ],
  "build": "python3 -m compileall .",
  "versionReporting": true,
  "pathMetadata": true,
  "assetMetadata": true,
  "staging": true,
  "output": "cdk.out",
  "lookups": true,
  "bundlingStacks": [],
  "notices": true,
  "rollback": true,
  "hotswap": false,
  "diff": {
    "contextLines": 3,
    "template": true,
    "fail": false
  },
  "deploy": {
    "rollback": true,
    "requireApproval": "never",
    "reuseAssets": true,
    "ignoreErrors": false,
    "toolkitStackName": "CDKToolkit",
    "parameters": {},
    "outputs": {}
  },
  "destroy": {
    "requireApproval": "never",
    "force": false
  },
  "metadata": {
    "analytics": true,
    "pathMetadata": true,
    "assetMetadata": true,
    "versionReporting": true,
    "stackTags": true
  }
}