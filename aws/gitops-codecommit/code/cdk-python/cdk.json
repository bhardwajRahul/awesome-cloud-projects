{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/.venv",
      "**/.env",
      "**/.DS_Store",
      "**/node_modules",
      "**/*.pyc",
      "**/*.pyo",
      "**/*.pyd",
      "**/.pytest_cache",
      "**/.coverage",
      "**/.mypy_cache",
      "**/htmlcov",
      "**/.tox",
      "**/venv",
      "**/ENV",
      "**/env",
      "**/.vscode",
      "**/.idea"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingForLambdaInvoke": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enforceHttps": true,
    "@aws-cdk/aws-s3:enforceSSL": true,
    "@aws-cdk/aws-s3:disallowInsecureConnections": true,
    "@aws-cdk/aws-glue:alphanumericNameGeneration": true,
    "@aws-cdk/aws-codepipeline:pipelineTypeV2": true,
    "@aws-cdk/aws-synthetics:enableAutoDeleteLambdas": true,
    "@aws-cdk/aws-kms:reduceCrossAccountRegionPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": false,
    "@aws-cdk/custom-resources:logRetentionLifetime": true,
    "@aws-cdk/aws-route53resolver:avoidDuplicateNamingErrorsOnDeleteThenCreate": true,
    "@aws-cdk/aws-route53-patterns:useCertificate": true,
    "@aws-cdk/aws-lambda-python:logRetentionLifetime": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:grantDecryptLambdaInsightRole": true,
    "@aws-cdk/aws-codepipeline-actions:variablesNamespace": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:grantDecryptLambdaInsightRole": true,
    "@aws-cdk/aws-elasticloadbalancingv2:trustStoreSupport": true,
    "@aws-cdk/aws-lambda:logRetentionLifetime": true,
    "@aws-cdk/aws-lambda-python:useLayerVersion": true,
    "@aws-cdk/aws-logs:logRetentionLifetime": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3UriParsing": true,
    "@aws-cdk/aws-route53resolver:enableLogging": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": false,
    "@aws-cdk/aws-ecs:enableServiceConnect": true,
    "@aws-cdk/aws-redshift:multiAzClusterParameterGroupSupport": true,
    "@aws-cdk/aws-stepfunctions-tasks:useNewS3UriParsing": true,
    "@aws-cdk/aws-route53resolver:enableLogging": true,
    "aws-cdk:enableDiffNoFail": "true",
    "aws-cdk:enablePathMetadata": "true",
    "aws-cdk:enableVersionReporting": "true",
    "aws-cdk:versionReporting": "true",
    "aws-cdk:target-partitions": ["aws", "aws-cn"],
    "project_name": "gitops-demo",
    "environment": "dev",
    "enable_monitoring": true,
    "enable_backup": false,
    "cost_center": "engineering",
    "owner": "gitops-team"
  },
  "toolkitStackName": "GitOpsWorkflowCDKToolkit",
  "profile": "default",
  "requireApproval": "broadening",
  "build": "echo 'No build required'",
  "diff": {
    "template": "cdk.out/GitOpsWorkflowStack-dev.template.json",
    "fail": false
  },
  "deploy": {
    "stacks": ["GitOpsWorkflowStack-dev"],
    "require-approval": "broadening",
    "rollback": true,
    "hotswap": false,
    "hotswap-fallback": true,
    "notification-arns": [],
    "parameters": {},
    "outputs-file": "cdk-outputs.json",
    "tags": {
      "Project": "GitOps-Workflows",
      "Environment": "Development",
      "ManagedBy": "CDK",
      "CostCenter": "Engineering"
    }
  },
  "destroy": {
    "stacks": ["GitOpsWorkflowStack-dev"],
    "force": false,
    "exclusively": false
  },
  "bootstrap": {
    "version": 20,
    "bucket-name": "gitops-cdk-bootstrap-${aws:account-id}-${aws:region}",
    "qualifier": "gitops",
    "public-access-block-configuration": true,
    "tags": {
      "Project": "GitOps-Workflows",
      "Environment": "Bootstrap",
      "ManagedBy": "CDK"
    }
  }
}