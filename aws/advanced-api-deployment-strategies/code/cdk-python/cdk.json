  "build": {
    "commands": [
      "pip install -r requirements.txt"
    ]
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "requireApproval": "never",
  "tags": {
    "Project": "AdvancedApiGatewayDeployment",
    "Environment": "Development",
    "Owner": "CDK",
    "Purpose": "BlueGreenCanaryDeployment"
  },
  "metadata": {
    "deployment-strategies": {
      "blue-green": {
        "enabled": true,
        "description": "Full environment swap deployment strategy"
      },
      "canary": {
        "enabled": true,
        "description": "Gradual traffic shifting deployment strategy",
        "defaultTrafficPercentage": 10
      }
    },
    "monitoring": {
      "cloudwatch": {
        "enabled": true,
        "logRetentionDays": 7
      },
      "alarms": {
        "enabled": true,
        "errorThreshold": 5,
        "latencyThreshold": 5000
      }
    }
  }
}