      "**/*.pyo",
      "**/.venv",
      "**/node_modules",
      "**/dist",
      "**/build",
      "**/*.egg-info",
      "**/.pytest_cache",
      "**/.coverage",
      "**/htmlcov",
      "**/.mypy_cache",
      "**/.tox",
      "**/cdk.out"
