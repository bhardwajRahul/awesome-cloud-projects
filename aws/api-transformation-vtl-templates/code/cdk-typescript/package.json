{
  "name": "request-response-transformation-vtl-templates-custom-models",
  "version": "1.0.0",
  "description": "CDK TypeScript application for Transforming API Requests with VTL Templates",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "ls": "cdk ls"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "api-gateway",
    "vtl",
    "lambda",
    "s3",
    "transformation",
    "request-response",
    "custom-models"
  ],
  "author": "AWS CDK Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/aws-cdk-examples"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "cdk": {
    "version": "2.100.0",
    "requireApproval": "never",
    "tags": {
      "project": "request-response-transformation",
      "environment": "demo"
    }
  }
}