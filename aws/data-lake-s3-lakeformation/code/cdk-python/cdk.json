{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.pytest_cache",
      "**/.coverage",
      "**/.tox",
      "**/.venv",
      "**/venv",
      "**/.git",
      "**/.gitignore",
      "**/*.egg-info",
      "**/build",
      "**/dist",
      "**/.mypy_cache",
      "**/.DS_Store",
      "**/Thumbs.db"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "@aws-cdk-containers/ecs-service-extensions:enableDefaultLogDriver": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizeWildcardPolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enableOpensearchMultiAzWithStandby": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-efs:mountTargetOrderInsensitiveLogicalId": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-codepipeline-actions:useNewDefaultBranchForCodeCommitSource": true,
    "@aws-cdk/aws-cloudwatch-actions:changeLambdaPermissionLogicalIdForLambdaAction": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeysDefaultValueToFalse": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:reduceCrossAccountRegionPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": true,
    "@aws-cdk/custom-resources:logApiResponseDataPropertyTrueDefault": false,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": false,
    "@aws-cdk/aws-ecs:reduceEc2FargateCloudWatchPermissions": true,
    "@aws-cdk/aws-lambda:enableLambdaProxyResourcePolicy": true,
    "@aws-cdk/aws-stepfunctions:stateMachineTypeDistributedSupport": true,
    "@aws-cdk/aws-logs:logGroupDeleteRetention": true,
    "@aws-cdk/aws-route53resolver:removeEmptyValidationRecords": true,
    "@aws-cdk/aws-events:generatedRuleIsManaged": true,
    "@aws-cdk/aws-events:eventBusNameAndTargetResourceId": true,
    "@aws-cdk/aws-s3:eventBridgeNotificationToSameEnvironment": true,
    "@aws-cdk/aws-apprunner:reduceSecurityGroupsPermissions": true,
    "@aws-cdk/aws-route53:enableTxtRecordConcatenation": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-ec2:ipamPoolPublicIpSource": true,
    "@aws-cdk/aws-ec2:skipDefaultNetworkAcl": true,
    "@aws-cdk/aws-ec2:skipDefaultSecurityGroup": true,
    "@aws-cdk/aws-cloudfront-origins:useDomainNameAsHttpHeaderName": true,
    "@aws-cdk/aws-cloudfront:removeDeprecatedCodecSupport": true,
    "@aws-cdk/aws-cloudfront:removeUnsupportedSecurityPolicyInOriginRequestPolicy": true,
    "@aws-cdk/aws-dynamodb:attributeDefinitionsFromProvider": true,
    "@aws-cdk/aws-logs:crossAccountLogGroupResourcePolicy": true,
    "@aws-cdk/aws-events:targetResourceId": true,
    "@aws-cdk/aws-apigateway:gatewayResponseTypeChange": true,
    "@aws-cdk/aws-route53:prefixWithRegion": true,
    "@aws-cdk/aws-route53:setWeightedTarget": true,
    "@aws-cdk/aws-route53:enableHostedZoneFqdn": true,
    "@aws-cdk/aws-route53:regionDrivenCaaEndpoint": true,
    "@aws-cdk/aws-ec2:allowOutboundTrafficToPrivateSubnets": true,
    "@aws-cdk/aws-s3:blockPublicAccess": true,
    "@aws-cdk/aws-cloudfront:originRequestPolicyHeaderBehavior": true,
    "@aws-cdk/aws-lambda:restrictSizeProp": true,
    "@aws-cdk/aws-logs:testLogEvents": true,
    "aws:cdk:enable-path-metadata": true,
    "aws:cdk:enable-diff-no-fail": true,
    "aws:cdk:version-reporting": true,
    "aws:cdk:stack-tags": true,
    "aws:cdk:asset-metadata": true,
    "aws:cdk:asset-hashing": true,
    "aws:cdk:bundling-stacks": []
  },
  "lookups": true,
  "versionReporting": true,
  "pathMetadata": true,
  "assetMetadata": true,
  "staging": true,
  "treeMetadata": true,
  "analyticsReporting": true,
  "requireApproval": "never",
  "toolkitStackName": "CDKToolkit",
  "toolkitBucketName": "cdktoolkit-stagingbucket-{randomSuffix}",
  "bundlingStacks": [],
  "notices": true,
  "assetPublishingExternalId": "cdk-asset-publishing-external-id",
  "assetPublishingRoleArn": "arn:aws:iam::{account}:role/cdk-asset-publishing-role-{randomSuffix}",
  "bootstrapKmsKeyId": "alias/aws/s3",
  "hotswap": false,
  "hotswapFallback": true,
  "profile": "default",
  "output": "cdk.out",
  "progress": "bar",
  "rollback": true,
  "stacks": "*",
  "staging": true,
  "tracingEnabled": false,
  "build": "npm run build",
  "language": "python",
  "feature_flags": {
    "@aws-cdk/aws-kms:defaultKeyPolicies": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true
  },
  "metadata": {
    "project": "data-lake-architecture",
    "description": "AWS CDK Python application for comprehensive data lake architecture with S3 and Lake Formation",
    "version": "1.0.0",
    "author": "Data Lake Team",
    "created": "2024-01-01",
    "lastModified": "2024-01-01",
    "tags": ["data-lake", "s3", "lake-formation", "glue", "analytics", "governance"],
    "dependencies": {
      "aws-cdk-lib": ">=2.120.0",
      "constructs": ">=10.3.0"
    },
    "deployment": {
      "regions": ["us-east-1", "us-west-2", "eu-west-1", "ap-southeast-1"],
      "environments": ["development", "staging", "production"]
    },
    "compliance": {
      "gdpr": true,
      "hipaa": true,
      "pci-dss": true,
      "sox": true
    }
  }
}