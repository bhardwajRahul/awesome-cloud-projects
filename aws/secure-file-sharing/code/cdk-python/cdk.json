    "toolkitStackName": "CDKToolkit",
    "template": null,
    "cfnExecutionPolicy": null,
    "trust": [],
    "trustedAccounts": [],
    "cloudFormationExecutionPolicies": [],
    "usePreviousParameters": true,
    "force": false
  },
  "synth": {
    "skipValidation": false,
    "json": false,
    "staging": true,
    "pathMetadata": false,
    "assetMetadata": false,
    "profile": null,
    "proxy": null,
    "lookups": true,
    "ignoreErrors": false,
    "strict": false,
