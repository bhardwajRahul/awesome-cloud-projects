{
  "app": "python app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/*.pyo",
      "**/*.pyd",
      ".venv/**",
      ".pytest_cache/**",
      ".mypy_cache/**",
      ".coverage",
      "htmlcov/**",
      "**/.DS_Store",
      "**/Thumbs.db"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk-containers/ecs-service-extensions:enableLogging": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/core:enablePartitionLiterals": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-iam:importedRoleStackSafeDefaultPolicyName": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-route53-patters:useCertificate": true,
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false,
    "@aws-cdk/aws-rds:databaseProxyUniqueResourceName": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-ec2:launchTemplateDefaultUserData": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/core:includePrefixInUniqueNameGeneration": true,
    "@aws-cdk/aws-opensearchservice:enableLogging": true,
    "@aws-cdk/aws-lambda-nodejs:useLatestRuntimeVersion": true,
    "@aws-cdk/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk/aws-opensearchservice:enforceHttps": true,
    "@aws-cdk/aws-s3:enforceSSLRequestsOnly": true,
    "@aws-cdk/aws-logs:logRetention": true,
    "@aws-cdk/aws-s3:defaultBucketSSEConfiguration": true,
    "@aws-cdk/aws-codebuild:privilegedModeDefaultEnvironment": true,
    "@aws-cdk/aws-codebuild:batchServiceRole": true,
    "@aws-cdk/aws-stepfunctions:stateMachineTypeDefault": "EXPRESS",
    "@aws-cdk/aws-cloudformation:checkCustomResourceNaming": true,
    "@aws-cdk/aws-cloudformation:crossStackResourceReferences": true,
    "@aws-cdk/aws-codepipeline:defaultPipelineTypeToV2": true,
    "@aws-cdk/aws-kms:reduceCrossAccountRegionPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:ebsDefaultGp3Volume": true,
    "@aws-cdk/aws-ecs:removeDefaultDeploymentAlarm": false,
    "@aws-cdk/custom-resources:logRetentionLogGroup": true,
    "@aws-cdk/aws-rds:setCorrectValueForDatabaseInstanceReadReplicaInstanceResourceId": true,
    "@aws-cdk/aws-route53-resolver:removeEmptyDnsFirewallRuleInRuleGroup": true,
    "@aws-cdk/aws-lambda-python:logLevel": "INFO",
    "project_name": "mobile-ab-testing",
    "enable_real_time_processing": "true",
    "enable_automated_analysis": "true",
    "deployment_environment": "development",
    "aws_region": "us-east-1",
    "pinpoint_project_settings": {
      "daily_message_limit": 1000,
      "messages_per_second": 100,
      "total_message_limit": 10000,
      "campaign_hook_enabled": true,
      "cloudwatch_metrics_enabled": true,
      "event_tagging_enabled": true
    },
    "analytics_settings": {
      "s3_lifecycle_enabled": true,
      "kinesis_retention_days": 1,
      "export_to_s3_enabled": true,
      "real_time_streaming_enabled": true
    },
    "lambda_settings": {
      "runtime_version": "python3.9",
      "memory_size": 512,
      "timeout_minutes": 5,
      "log_retention_days": 14,
      "enable_xray_tracing": true
    },
    "monitoring_settings": {
      "dashboard_enabled": true,
      "alarms_enabled": true,
      "bounce_rate_threshold": 100,
      "delivery_rate_threshold": 90,
      "notification_enabled": false
    },
    "security_settings": {
      "kms_key_rotation_enabled": true,
      "s3_public_access_blocked": true,
      "least_privilege_iam": true,
      "encryption_in_transit": true,
      "encryption_at_rest": true
    },
    "cost_optimization": {
      "s3_lifecycle_rules_enabled": true,
      "kinesis_auto_scaling_enabled": false,
      "lambda_reserved_concurrency": false,
      "cloudwatch_log_retention_days": 14
    },
    "ab_test_configuration": {
      "control_percentage": 10,
      "treatment_a_percentage": 45,
      "treatment_b_percentage": 45,
      "holdout_percentage": 10,
      "statistical_significance_threshold": 0.05,
      "minimum_sample_size": 100
    },
    "segment_configuration": {
      "active_users_app_versions": ["1.0.0", "1.1.0", "1.2.0"],
      "high_value_users_session_threshold": 5,
      "recency_window_days": 7,
      "segment_refresh_frequency": "daily"
    }
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "requireApproval": "never",
  "output": "cdk.out",
  "pathMetadata": true,
  "assetMetadata": true,
  "versionReporting": true,
  "analyticsReporting": true,
  "staging": true,
  "bundlingStacks": [],
  "lookups": true,
  "notices": true,
  "build": {
    "commands": [
      "pip install -r requirements.txt",
      "python -m pytest tests/ -v",
      "python -m black --check .",
      "python -m flake8 .",
      "python -m mypy ."
    ]
  },
  "deploy": {
    "requireApproval": "never",
    "parameters": {
      "EnableRealTimeProcessing": "true",
      "EnableAutomatedAnalysis": "true",
      "ProjectName": "mobile-ab-testing"
    }
  }
}