{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      "**/.venv",
      "**/.mypy_cache",
      "**/.pytest_cache",
      "**/node_modules",
      "**/*.egg-info",
      "**/.DS_Store"
    ]
  },
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": true,
    "@aws-cdk/core:stackRelativeExports": true,
    "@aws-cdk/core:target-partitions": [
      "aws",
      "aws-cn"
    ],
    "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk/aws-efs:defaultEncryptionAtRest": true,
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk/aws-codedeploy:removeAlarmsFromDeploymentGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-kms:aliasNameRef": true,
    "@aws-cdk/aws-autoscaling:generateLaunchTemplateInsteadOfLaunchConfig": true,
    "@aws-cdk/aws-lambda:checkSecurityGroupRules": true,
    "@aws-cdk/aws-iam:minimizePolicies": true,
    "@aws-cdk/aws-route53:splitHorizonOptimization": true,
    "@aws-cdk/aws-ssm:parameterValueSelect": true,
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/aws-lambda:recognizeLambdaEnvironmentVariables": true,
    "@aws-cdk/aws-apigateway:requiredValidatorFields": true,
    "@aws-cdk/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk/aws-ecs:disableExplicitDeploymentControllerForCircuitBreaker": true,
    "@aws-cdk/aws-s3:enforceSSLRequestsOnly": true,
    "@aws-cdk/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk/aws-apigateway:disableCloudWatchRoleV2": true,
    "@aws-cdk/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk/aws-iam:importedRoleStackSafe": true,
    "@aws-cdk/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk/aws-iam:principalNameLowerCase": true,
    "@aws-cdk/aws-lambda:invokeFunctionPermissions": true,
    "@aws-cdk/aws-codedeploy:deploymentGroupECSTargetTypeOptIn": true,
    "@aws-cdk/aws-route53resolver:queryLoggingDefaultDestination": true,
    "@aws-cdk/aws-autoscaling:enableLaunchTemplateSpotAllocation": true,
    "@aws-cdk/aws-route53:moveTargetRecordBetweenHostedZones": true,
    "@aws-cdk/aws-autoscaling:useDesiredCapacityAsDefaultCapacity": true,
    "@aws-cdk/aws-lambda:usingZippedLambdaAssetsOptIn": true,
    "@aws-cdk/aws-s3:serverAccessLogsUseBucketPolicy": true,
    "@aws-cdk/aws-apigateway:managedResourcePolicyOptIn": true,
    "@aws-cdk/aws-secretsmanager:useAttachedSecretResourcePolicyForSecretTargetAttachments": true,
    "@aws-cdk/aws-redshift:columnId": true,
    "@aws-cdk/aws-stepfunctions-tasks:enableLoggingSfnExecution": true,
    "@aws-cdk/aws-kms:reduceCrossAccountRegionPolicyScope": true,
    "@aws-cdk/aws-eks:nodegroupNameAttribute": true,
    "@aws-cdk/aws-ec2:restrictDefaultSecurityGroup": true,
    "@aws-cdk/aws-apigateway:requestValidatorUniqueId": true,
    "@aws-cdk/aws-elasticloadbalancingv2:enableLogs": true,
    "@aws-cdk/aws-s3:enforceSSLRequestsOnly": true,
    "@aws-cdk/aws-s3:objectLockDefaultRetention": true,
    "@aws-cdk/aws-codebuild:enableLoggingV2": true,
    "@aws-cdk/aws-codebuild:useLinuxBuildImage": true,
    "@aws-cdk/aws-s3:bucketGrantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk/aws-ecr-assets:duplicateTagRemovalStrategy": true,
    "@aws-cdk/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk/aws-route53:useNatGatewayRouteTableReference": true,
    "@aws-cdk/aws-apprunner:useImageTag": true,
    "@aws-cdk/aws-lambda:lambdaNodejs18.x": true,
    "@aws-cdk/aws-lambda:lambdaNodejs20.x": true,
    "@aws-cdk/aws-codedeploy:codeDeployRoleSemantics": true,
    "@aws-cdk/aws-apigateway:authorizerChangeDeploymentLogicalId": true,
    "@aws-cdk/aws-sqs:useLongPollingForLogicalId": true,
    "@aws-cdk/aws-lambda:removeResourcePolicyWhenSamOrigin": true,
    "@aws-cdk/aws-stepfunctions:useLongformStateMachineType": true,
    "@aws-cdk/aws-lambda:reportCodeSourceMapping": true,
    "@aws-cdk/aws-s3:keepNotificationInImportedBucket": true,
    "@aws-cdk/aws-events:makeRuleOptional": true,
    "@aws-cdk/aws-sns:validateSubscriptionInlinePolicies": true,
    "@aws-cdk/aws-s3:bucketNotificationsHandlerRolePolicy": true,
    "@aws-cdk/aws-elasticloadbalancingv2:enableNLBStickinessCookie": true,
    "@aws-cdk/aws-lambda:pythonVersion": "3.12"
  },
  "feature_flags": {
    "@aws-cdk/aws-ssm:enableDefaultPatchBaselines": true,
    "@aws-cdk/aws-cloudwatch:enableInsightRules": true,
    "@aws-cdk/aws-ec2:enableDnsHostnames": true,
    "@aws-cdk/aws-ec2:enableDnsSupport": true,
    "@aws-cdk/aws-iam:enableResourceNameLowerCase": true,
    "@aws-cdk/aws-s3:failOnNoObjectsInBucket": true,
    "@aws-cdk/aws-lambda:enableAutoPublishAlias": true,
    "@aws-cdk/aws-sns:enableTopicBillingMode": true,
    "@aws-cdk/aws-ssm:useSsmParameterNameLowerCase": true
  },
  "toolkitStackName": "CDKToolkit",
  "profile": "default",
  "requireApproval": "never",
  "assetMetadata": true,
  "pathMetadata": true,
  "output": "cdk.out",
  "bundlingStacks": [],
  "lookups": true,
  "notices": true,
  "diff": true,
  "versionReporting": true,
  "staging": true,
  "plugin": []
}